package javabook;

public class Page065 {

	public static void main(String[] args) {
		double x = 0;
		
		x = (int)3.6 + (int)3.3;
		System.out.println("a. 실수 : " + x + ", 정수 : " + (int)x);

		x = (int)((2+3)*10.5);
		System.out.println("b. 실수 : " + x + ", 정수 : " + (int)x);
		
		x = 3/5*22.0;
		System.out.println("b. 실수 : " + x + ", 정수 : " + (int)x);
				
	}

}
